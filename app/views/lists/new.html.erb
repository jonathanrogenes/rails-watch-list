<div class="container">
  <h1>Create a list</h1>
  <%= simple_form_for @list do |f| %>
    <%= f.input :name %>
    <%= f.submit 'Submit', class: 'btn btn-primary' %>
  <% end %>

  <%= link_to "Return Home", lists_path, class: "btn border mt-3 shadow-sm text-dark px-3 text-decoration-none bg-light rounded" %>
</div>
